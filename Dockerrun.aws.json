{
    "AWSEBDockerrunversion":"2",
    "containerDefinitions":[
        {
            "name":"client",
            "image":"anujjaryal/multi-client",
            "hostname":"client",
            "essential":false
        },
        {
            "name":"server",
            "image":"anujjaryal/multi-server",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"anujjaryal/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"anujjaryal/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":[
                "client",
                "server"
            ]
        }
    ]
}